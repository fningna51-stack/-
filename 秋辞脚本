PlayerTab:Button({Title="透视",Callback=function()
	local FlatIdent_276C2 = 0;
	local FillColor;
	local DepthMode;
	local FillTransparency;
	local OutlineColor;
	local OutlineTransparency;
	local CoreGui;
	local Players;
	local lp;
	local connections;
	local Storage;
	local Highlight;
	while true do
		if (FlatIdent_276C2 == 0) then
			FillColor = Color3.fromRGB(175, 25, 255);
			DepthMode = "AlwaysOnTop";
			FillTransparency = 0.5;
			OutlineColor = Color3.fromRGB(255, 255, 255);
			FlatIdent_276C2 = 1;
		end
		if (FlatIdent_276C2 == 2) then
			connections = {};
			Storage = Instance.new("Folder");
			Storage.Parent = CoreGui;
			Storage.Name = "Highlight_Storage";
			FlatIdent_276C2 = 3;
		end
		if (FlatIdent_276C2 == 1) then
			OutlineTransparency = 0;
			CoreGui = game:FindService("CoreGui");
			Players = game:FindService("Players");
			lp = Players.LocalPlayer;
			FlatIdent_276C2 = 2;
		end
		if (FlatIdent_276C2 == 3) then
			Highlight = nil;
			function Highlight(plr)
				local Highlight = Instance.new("Highlight");
				Highlight.Name = plr.Name;
				Highlight.FillColor = FillColor;
				Highlight.DepthMode = DepthMode;
				Highlight.FillTransparency = FillTransparency;
				Highlight.OutlineColor = OutlineColor;
				Highlight.OutlineTransparency = 0;
				Highlight.Parent = Storage;
				local plrchar = plr.Character;
				if plrchar then
					Highlight.Adornee = plrchar;
				end
				connections[plr] = plr.CharacterAdded:Connect(function(char)
					Highlight.Adornee = char;
				end);
			end
			Players.PlayerAdded:Connect(Highlight);
			for i, v in next, Players:GetPlayers() do
				Highlight(v);
			end
			FlatIdent_276C2 = 4;
		end
		if (4 == FlatIdent_276C2) then
			Players.PlayerRemoving:Connect(function(plr)
				local plrname = plr.Name;
				if Storage[plrname] then
					Storage[plrname]:Destroy();
				end
				if connections[plr] then
					connections[plr]:Disconnect();
				end
			end);
			WindUI:Notify({Title="透视已开启",Content="玩家透视已启用",Duration=3});
			break;
		end
	end
end});
PlayerTab:Button({Title="重置角色",Callback=function()
	local FlatIdent_7873D = 0;
	local player;
	while true do
		if (1 == FlatIdent_7873D) then
			WindUI:Notify({Title="重置成功",Content="角色已重置",Duration=2});
			break;
		end
		if (FlatIdent_7873D == 0) then
			player = game.Players.LocalPlayer;
			if player.Character then
				player.Character:BreakJoints();
			end
			FlatIdent_7873D = 1;
		end
	end
end});
