local particleTime = 0;
RunService.RenderStepped:Connect(function(dt)
	particleTime = particleTime + dt;
	for _, particle in ipairs(particles) do
		local FlatIdent_2E9CB = 0;
		local frame;
		local currentPos;
		local newX;
		local newY;
		local pulse;
		local transparency;
		local colorOffset;
		while true do
			if (FlatIdent_2E9CB == 4) then
				frame.BackgroundColor3 = Color3.fromRGB(math.clamp(particle.color.R * 255 * (1 + colorOffset), 150, 255), math.clamp(particle.color.G * 255 * (1 + colorOffset), 150, 255), math.clamp(particle.color.B * 255 * (1 + colorOffset), 150, 255));
				break;
			end
			if (FlatIdent_2E9CB == 3) then
				transparency = 0.4 + (math.sin((particleTime * 1.2) + _) * 0.3);
				frame.BackgroundTransparency = transparency;
				colorOffset = math.sin((particleTime * 0.8) + _) * 0.2;
				FlatIdent_2E9CB = 4;
			end
			if (2 == FlatIdent_2E9CB) then
				pulse = (math.sin((particleTime * 1.5) + _) * 0.25) + 0.9;
				frame.Size = UDim2.new(0, particle.size.X.Offset * pulse, 0, particle.size.Y.Offset * pulse);
				frame.Position = UDim2.new(newX, 0, newY, 0);
				FlatIdent_2E9CB = 3;
			end
			if (FlatIdent_2E9CB == 1) then
				newY = currentPos.Y.Scale + (particle.speedY * dt * 0.8);
				if ((newX < -0.05) or (newX > 0.95)) then
					local FlatIdent_6A091 = 0;
					while true do
						if (FlatIdent_6A091 == 0) then
							particle.speedX = -particle.speedX;
							newX = math.clamp(newX, -0.05, 0.95);
							break;
						end
					end
				end
				if ((newY < -0.05) or (newY > 0.95)) then
					local FlatIdent_882F4 = 0;
					while true do
						if (FlatIdent_882F4 == 0) then
							particle.speedY = -particle.speedY;
							newY = math.clamp(newY, -0.05, 0.95);
							break;
						end
					end
				end
				FlatIdent_2E9CB = 2;
			end
			if (0 == FlatIdent_2E9CB) then
				frame = particle.frame;
				currentPos = frame.Position;
				newX = currentPos.X.Scale + (particle.speedX * dt * 0.8);
				FlatIdent_2E9CB = 1;
			end
		end
	end
end);
