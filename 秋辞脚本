AddScriptButton(PlayerTab, "无敌", function()
	local lp = game:GetService("Players").LocalPlayer;
	if lp.Character:FindFirstChild("Head") then
		local FlatIdent_8ABD6 = 0;
		local char;
		local new;
		local oldhum;
		local newhum;
		while true do
			if (FlatIdent_8ABD6 == 0) then
				char = lp.Character;
				char.Archivable = true;
				new = char:Clone();
				new.Parent = workspace;
				FlatIdent_8ABD6 = 1;
			end
			if (FlatIdent_8ABD6 == 2) then
				newhum.Parent = char;
				newhum.RequiresNeck = false;
				oldhum.Parent = nil;
				wait(2);
				FlatIdent_8ABD6 = 3;
			end
			if (FlatIdent_8ABD6 == 3) then
				lp.Character = char;
				new:Destroy();
				wait(1);
				newhum:GetPropertyChangedSignal("Health"):Connect(function()
					if (newhum.Health <= 0) then
						local FlatIdent_21297 = 0;
						while true do
							if (FlatIdent_21297 == 0) then
								oldhum.Parent = lp.Character;
								wait(1);
								FlatIdent_21297 = 1;
							end
							if (FlatIdent_21297 == 1) then
								oldhum:Destroy();
								break;
							end
						end
					end
				end);
				FlatIdent_8ABD6 = 4;
			end
			if (FlatIdent_8ABD6 == 1) then
				lp.Character = new;
				wait(2);
				oldhum = char:FindFirstChildWhichIsA("Humanoid");
				newhum = oldhum:Clone();
				FlatIdent_8ABD6 = 2;
			end
			if (FlatIdent_8ABD6 == 4) then
				workspace.CurrentCamera.CameraSubject = char;
				if char:FindFirstChild("Animate") then
					local FlatIdent_28855 = 0;
					while true do
						if (FlatIdent_28855 == 1) then
							char.Animate.Disabled = false;
							break;
						end
						if (FlatIdent_28855 == 0) then
							char.Animate.Disabled = true;
							wait(0.1);
							FlatIdent_28855 = 1;
						end
					end
				end
				lp.Character:FindFirstChild("Head"):Destroy();
				break;
			end
		end
	end
end);
