local WalkSpeedValue = 16;
local walkSpeedSlider;
walkSpeedSlider = PlayerTab:Slider({Title="移动速度",Value={Min=16,Max=200,Default=16},Suffix=" 速度",Callback=function(value)
	WalkSpeedValue = value;
	local player = game.Players.LocalPlayer;
	if player.Character then
		local FlatIdent_3CF01 = 0;
		local humanoid;
		while true do
			if (FlatIdent_3CF01 == 0) then
				humanoid = player.Character:FindFirstChildOfClass("Humanoid");
				if humanoid then
					humanoid.WalkSpeed = value;
				end
				break;
			end
		end
	end
end});
local JumpPowerValue = 50;
local jumpPowerSlider;
jumpPowerSlider = PlayerTab:Slider({Title="跳跃高度",Value={Min=50,Max=200,Default=50},Suffix=" 高度",Callback=function(value)
	JumpPowerValue = value;
	local player = game.Players.LocalPlayer;
	if player.Character then
		local FlatIdent_5EE26 = 0;
		local humanoid;
		while true do
			if (FlatIdent_5EE26 == 0) then
				humanoid = player.Character:FindFirstChildOfClass("Humanoid");
				if humanoid then
					humanoid.JumpPower = value;
				end
				break;
			end
		end
	end
end});
local CharacterSizeValue = "正常";
PlayerTab:Dropdown({Title="角色大小",Values={"正常","巨人","迷你"},Value="正常",Callback=function(option)
	local FlatIdent_32B97 = 0;
	local player;
	while true do
		if (FlatIdent_32B97 == 0) then
			CharacterSizeValue = option;
			player = game.Players.LocalPlayer;
			FlatIdent_32B97 = 1;
		end
		if (FlatIdent_32B97 == 1) then
			if player.Character then
				local scale = 1;
				if (option == "巨人") then
					scale = 2;
				elseif (option == "迷你") then
					scale = 0.5;
				end
				local humanoid = player.Character:FindFirstChildOfClass("Humanoid");
				if humanoid then
					local FlatIdent_580CB = 0;
					local setScale;
					while true do
						if (FlatIdent_580CB == 1) then
							for _, part in pairs(player.Character:GetDescendants()) do
								pcall(setScale, part);
							end
							break;
						end
						if (FlatIdent_580CB == 0) then
							setScale = nil;
							function setScale(part)
								if part:IsA("BasePart") then
									part.Size = part.Size * scale;
								end
							end
							FlatIdent_580CB = 1;
						end
					end
				end
			end
			WindUI:Notify({Title="角色大小已设置",Content=("当前大小: " .. option),Duration=2});
			break;
		end
	end
end});
local NightVisionToggle;
NightVisionToggle = PlayerTab:Toggle({Title="夜视",Value=false,Callback=function(value)
	if value then
		game.Lighting.Ambient = Color3.new(1, 1, 1);
	else
		game.Lighting.Ambient = Color3.new(0, 0, 0);
	end
end});
