local function AddScriptButton(tab, text, funcOrUrl, description)
	tab:Button({Title=text,Callback=function()
		if (type(funcOrUrl) == "function") then
			funcOrUrl();
		else
			loadstring(game:HttpGet(funcOrUrl, true))();
		end
		WindUI:Notify({Title="加载成功",Content=(description or (text .. "已加载")),Duration=3});
	end});
end
