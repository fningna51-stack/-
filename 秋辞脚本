local HttpService = game:GetService("HttpService");
local folderPath = "QC_Script_Configs";
local function ensureFolder()
	local FlatIdent_C595 = 0;
	while true do
		if (FlatIdent_C595 == 1) then
			return true;
		end
		if (FlatIdent_C595 == 0) then
			if not isfolder then
				local FlatIdent_90A69 = 0;
				while true do
					if (FlatIdent_90A69 == 0) then
						WindUI:Notify({Title="警告",Content="配置文件功能不可用",Duration=5});
						return false;
					end
				end
			end
			if not isfolder(folderPath) then
				makefolder(folderPath);
			end
			FlatIdent_C595 = 1;
		end
	end
end
local function SaveFile(fileName, data)
	if not ensureFolder() then
		return;
	end
	local filePath = folderPath .. "/" .. fileName .. ".json";
	local jsonData = HttpService:JSONEncode(data);
	writefile(filePath, jsonData);
	WindUI:Notify({Title="保存成功",Content=("配置已保存到: " .. filePath),Duration=3});
end
local function LoadFile(fileName)
	local FlatIdent_1BB5D = 0;
	local filePath;
	while true do
		if (FlatIdent_1BB5D == 1) then
			if isfile(filePath) then
				local FlatIdent_13B77 = 0;
				local jsonData;
				while true do
					if (0 == FlatIdent_13B77) then
						jsonData = readfile(filePath);
						return HttpService:JSONDecode(jsonData);
					end
				end
			end
			return nil;
		end
		if (FlatIdent_1BB5D == 0) then
			if not ensureFolder() then
				return nil;
			end
			filePath = folderPath .. "/" .. fileName .. ".json";
			FlatIdent_1BB5D = 1;
		end
	end
end
local function ListFiles()
	local FlatIdent_94CF9 = 0;
	local files;
	while true do
		if (FlatIdent_94CF9 == 1) then
			for _, file in ipairs(listfiles(folderPath)) do
				local FlatIdent_8B7B0 = 0;
				local fileName;
				while true do
					if (FlatIdent_8B7B0 == 0) then
						fileName = file:match("([^/]+)%.json$");
						if fileName then
							table.insert(files, fileName);
						end
						break;
					end
				end
			end
			return files;
		end
		if (FlatIdent_94CF9 == 0) then
			if not ensureFolder() then
				return {};
			end
			files = {};
			FlatIdent_94CF9 = 1;
		end
	end
end
SettingsTab:Section({Title="配置管理",Icon="save"});
local configName = "";
SettingsTab:Input({Title="配置名称",Placeholder="输入配置名称",Callback=function(text)
	configName = text;
end});
SettingsTab:Button({Title="保存配置",Icon="save",Callback=function()
	if ((configName ~= "") and (configName ~= "")) then
		local configData = {WalkSpeed=WalkSpeedValue,JumpPower=JumpPowerValue,CharacterSize=CharacterSizeValue,NightVision=NightVisionToggle:GetValue(),Theme=WindUI:GetCurrentTheme(),SpinSpeed=spinSpeed};
		SaveFile(configName, configData);
	else
		WindUI:Notify({Title="错误",Content="请输入配置名称",Duration=3});
	end
end});
SettingsTab:Button({Title="加载最近配置",Icon="folder-open",Callback=function()
	local FlatIdent_35AC5 = 0;
	local files;
	while true do
		if (FlatIdent_35AC5 == 0) then
			files = ListFiles();
			if (#files > 0) then
				local FlatIdent_506A5 = 0;
				local recentFile;
				local data;
				while true do
					if (FlatIdent_506A5 == 0) then
						recentFile = files[#files];
						data = LoadFile(recentFile);
						FlatIdent_506A5 = 1;
					end
					if (FlatIdent_506A5 == 1) then
						if data then
							local FlatIdent_1454F = 0;
							while true do
								if (FlatIdent_1454F == 2) then
									if data.SpinSpeed then
										spinSpeed = data.SpinSpeed;
									end
									WindUI:Notify({Title="配置已加载",Content=("从 " .. recentFile .. " 加载配置"),Duration=3});
									break;
								end
								if (FlatIdent_1454F == 0) then
									if data.WalkSpeed then
										local FlatIdent_52EE1 = 0;
										while true do
											if (FlatIdent_52EE1 == 0) then
												WalkSpeedValue = data.WalkSpeed;
												walkSpeedSlider:SetValue(data.WalkSpeed);
												break;
											end
										end
									end
									if data.JumpPower then
										local FlatIdent_2DB3E = 0;
										while true do
											if (FlatIdent_2DB3E == 0) then
												JumpPowerValue = data.JumpPower;
												jumpPowerSlider:SetValue(data.JumpPower);
												break;
											end
										end
									end
									FlatIdent_1454F = 1;
								end
								if (FlatIdent_1454F == 1) then
									if data.CharacterSize then
										CharacterSizeValue = data.CharacterSize;
									end
									if (data.NightVision ~= nil) then
										NightVisionToggle:SetValue(data.NightVision);
									end
									FlatIdent_1454F = 2;
								end
							end
						end
						break;
					end
				end
			else
				WindUI:Notify({Title="无配置文件",Content="没有找到配置文件",Duration=3});
			end
			break;
		end
	end
end});
