local DfnmTab = Tabs.DfnmTab:Tab({Title="旋转控制",Icon="refresh-cw"});
DfnmTab:Paragraph({Title="旋转控制",Desc="控制角色旋转速度和方向",Image="refresh-cw"});
local angularVel = nil;
local spinSpeed = 0;
DfnmTab:Slider({Title="旋转速度",Value={Min=0,Max=500,Default=0},Suffix=" 速度",Callback=function(value)
	spinSpeed = value;
	local char = game.Players.LocalPlayer.Character;
	if not char then
		return;
	end
	local root = char:FindFirstChild("HumanoidRootPart");
	if not root then
		return;
	end
	if angularVel then
		local FlatIdent_92F66 = 0;
		while true do
			if (FlatIdent_92F66 == 0) then
				angularVel:Destroy();
				angularVel = nil;
				break;
			end
		end
	end
	if (value > 0) then
		local FlatIdent_957A4 = 0;
		while true do
			if (FlatIdent_957A4 == 0) then
				angularVel = Instance.new("BodyAngularVelocity");
				angularVel.Name = "CustomSpin";
				FlatIdent_957A4 = 1;
			end
			if (FlatIdent_957A4 == 1) then
				angularVel.MaxTorque = Vector3.new(0, math.huge, 0);
				angularVel.AngularVelocity = Vector3.new(0, value, 0);
				FlatIdent_957A4 = 2;
			end
			if (FlatIdent_957A4 == 2) then
				angularVel.P = 1250;
				angularVel.Parent = root;
				break;
			end
		end
	end
end});
local spinSpeeds = {10,20,30,50,100,200,500};
local speedButtonsGroup = {};
for _, speed in ipairs(spinSpeeds) do
	DfnmTab:Button({Title=("旋转 " .. speed),Callback=function()
		spinSpeed = speed;
		local char = game.Players.LocalPlayer.Character;
		if not char then
			return;
		end
		local root = char:FindFirstChild("HumanoidRootPart");
		if not root then
			return;
		end
		if angularVel then
			angularVel:Destroy();
			angularVel = nil;
		end
		angularVel = Instance.new("BodyAngularVelocity");
		angularVel.Name = "CustomSpin";
		angularVel.MaxTorque = Vector3.new(0, math.huge, 0);
		angularVel.AngularVelocity = Vector3.new(0, speed, 0);
		angularVel.P = 1250;
		angularVel.Parent = root;
		WindUI:Notify({Title="旋转速度已设置",Content=("当前速度: " .. speed),Duration=2});
	end});
end
DfnmTab:Button({Title="停止旋转",Callback=function()
	spinSpeed = 0;
	if angularVel then
		local FlatIdent_6E549 = 0;
		while true do
			if (FlatIdent_6E549 == 0) then
				angularVel:Destroy();
				angularVel = nil;
				break;
			end
		end
	end
	WindUI:Notify({Title="旋转已停止",Content="角色停止旋转",Duration=2});
end});
DfnmTab:Toggle({Title="反转旋转方向",Value=false,Callback=function(value)
	if angularVel then
		local FlatIdent_21CA5 = 0;
		local currentSpeed;
		while true do
			if (FlatIdent_21CA5 == 0) then
				currentSpeed = spinSpeed;
				if value then
					angularVel.AngularVelocity = Vector3.new(0, -currentSpeed, 0);
				else
					angularVel.AngularVelocity = Vector3.new(0, currentSpeed, 0);
				end
				break;
			end
		end
	end
end});
DfnmTab:Toggle({Title="自动应用旋转",Desc="死亡重生后自动重新应用旋转",Value=false,Callback=function(value)
	if value then
		local FlatIdent_7517F = 0;
		local player;
		local applyRotation;
		while true do
			if (FlatIdent_7517F == 1) then
				function applyRotation()
					local FlatIdent_55D83 = 0;
					while true do
						if (FlatIdent_55D83 == 0) then
							task.wait(1);
							if (spinSpeed > 0) then
								local FlatIdent_6C967 = 0;
								local char;
								while true do
									if (FlatIdent_6C967 == 0) then
										char = player.Character;
										if char then
											local FlatIdent_2FBBD = 0;
											local root;
											while true do
												if (0 == FlatIdent_2FBBD) then
													root = char:FindFirstChild("HumanoidRootPart");
													if root then
														local FlatIdent_90113 = 0;
														while true do
															if (FlatIdent_90113 == 0) then
																if angularVel then
																	local FlatIdent_6DFD9 = 0;
																	while true do
																		if (FlatIdent_6DFD9 == 0) then
																			angularVel:Destroy();
																			angularVel = nil;
																			break;
																		end
																	end
																end
																angularVel = Instance.new("BodyAngularVelocity");
																FlatIdent_90113 = 1;
															end
															if (FlatIdent_90113 == 3) then
																angularVel.Parent = root;
																break;
															end
															if (FlatIdent_90113 == 1) then
																angularVel.Name = "CustomSpin";
																angularVel.MaxTorque = Vector3.new(0, math.huge, 0);
																FlatIdent_90113 = 2;
															end
															if (FlatIdent_90113 == 2) then
																angularVel.AngularVelocity = Vector3.new(0, spinSpeed, 0);
																angularVel.P = 1250;
																FlatIdent_90113 = 3;
															end
														end
													end
													break;
												end
											end
										end
										break;
									end
								end
							end
							break;
						end
					end
				end
				player.CharacterAdded:Connect(applyRotation);
				break;
			end
			if (0 == FlatIdent_7517F) then
				player = game.Players.LocalPlayer;
				applyRotation = nil;
				FlatIdent_7517F = 1;
			end
		end
	end
end});
