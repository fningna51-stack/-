SettingsTab:Button({Title="重置所有设置",Icon="trash-2",Variant="Secondary",Callback=function()
	local FlatIdent_3501F = 0;
	local player;
	while true do
		if (FlatIdent_3501F == 3) then
			NightVisionToggle:SetValue(false);
			WindUI:Notify({Title="已重置所有设置",Content="所有设置已恢复默认值",Duration=3});
			break;
		end
		if (2 == FlatIdent_3501F) then
			if player.Character then
				local FlatIdent_61084 = 0;
				local humanoid;
				while true do
					if (FlatIdent_61084 == 0) then
						humanoid = player.Character:FindFirstChildOfClass("Humanoid");
						if humanoid then
							humanoid.JumpPower = 50;
						end
						break;
					end
				end
			end
			if angularVel then
				local FlatIdent_69D54 = 0;
				while true do
					if (FlatIdent_69D54 == 0) then
						angularVel:Destroy();
						angularVel = nil;
						break;
					end
				end
			end
			spinSpeed = 0;
			FlatIdent_3501F = 3;
		end
		if (1 == FlatIdent_3501F) then
			if player.Character then
				local FlatIdent_23FF9 = 0;
				local humanoid;
				while true do
					if (FlatIdent_23FF9 == 0) then
						humanoid = player.Character:FindFirstChildOfClass("Humanoid");
						if humanoid then
							humanoid.WalkSpeed = 16;
						end
						break;
					end
				end
			end
			JumpPowerValue = 50;
			jumpPowerSlider:SetValue(50);
			FlatIdent_3501F = 2;
		end
		if (FlatIdent_3501F == 0) then
			WalkSpeedValue = 16;
			walkSpeedSlider:SetValue(16);
			player = game.Players.LocalPlayer;
			FlatIdent_3501F = 1;
		end
	end
end});
