local WindUI = loadstring(game:HttpGet("https://github.com/Footagesus/WindUI/releases/latest/download/main.lua"))()

local Window = WindUI:CreateWindow({
    Title = "秋风脚本重置版",
    Icon = "door-open",
    Author = "",
    Folder = "MyScriptHub",
    Size = UDim2.fromOffset(560, 360),
    Transparent = true,
    Theme = "Dark",
    User = {
        Enabled = true,
        Callback = function() 
            print("用户信息被点击") 
        end,
        Anonymous = true
    },
})

Window:EditOpenButton({
    Title = "打开秋风脚本重置版",
    Icon = "monitor",
    CornerRadius = UDim.new(0,16),
    StrokeThickness = 2,
    Color = ColorSequence.new(
        Color3.fromHex("FF0F7B"),
        Color3.fromHex("F89B29")
    ),
    Draggable = true,
})

-- 辅助函数定义
function Tab(a)
    return Window:Tab({Title = a, Icon = "eye"})
end

function Button(a, b, c)
    return a:Button({Title = b, Callback = c})
end

function Toggle(a, b, c, d)
    return a:Toggle({Title = b, Value = c, Callback = d})
end

function Slider(a, b, c, d, e, f)
    return a:Slider({Title = b, Step = 1, Value = {Min = c, Max = d, Default = e}, Callback = f})
end

function Dropdown(a, b, c, d, e)
    return a:Dropdown({Title = b, Values = c, Value = d, Callback = e})
end

-- 创建标签页
local MainTab = Tab("主要功能")
local PlayerTab = Tab("玩家设置")
local GameTab = Tab("游戏功能")

-- 主要功能标签页
Button(MainTab, "飞行脚本", function()
    loadstring(game:HttpGet("https://example.com/flyscript.lua"))()
    print("飞行脚本已加载")
end)

Button(MainTab, "穿墙功能", function()
    loadstring(game:HttpGet("https://example.com/noclip.lua"))()
    print("穿墙功能已启用")
end)

-- 玩家设置标签页
Slider(PlayerTab, "移动速度", 16, 200, game.Players.LocalPlayer.Character.Humanoid.WalkSpeed, function(value)
    game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = value
    print("移动速度设置为: " .. value)
end)

Slider(PlayerTab, "跳跃高度", 50, 500, game.Players.LocalPlayer.Character.Humanoid.JumpPower, function(value)
    game.Players.LocalPlayer.Character.Humanoid.JumpPower = value
    print("跳跃高度设置为: " .. value)
end)

Toggle(PlayerTab, "夜视模式", false, function(state)
    if state then
        game.Lighting.Ambient = Color3.new(1, 1, 1)
        print("夜视模式已开启")
    else
        game.Lighting.Ambient = Color3.new(0, 0, 0)
        print("夜视模式已关闭")
    end
end)

-- 游戏功能标签页
local games = {
    "力量传奇",
    "Brookhaven",
    "Arsenal",
    "Piggy"
}

Dropdown(GameTab, "选择游戏", games, "", function(selected)
    print("已选择游戏: " .. selected)
    
    if selected == "力量传奇" then
        Button(GameTab, "加载力量传奇脚本", function()
            loadstring(game:HttpGet("https://example.com/strengthlegend.lua"))()
        end)
    elseif selected == "Brookhaven" then
        Button(GameTab, "加载Brookhaven脚本", function()
            loadstring(game:HttpGet("https://example.com/brookhaven.lua"))()
        end)
    end
end)

Button(GameTab, "自动农场", function()
    loadstring(game:HttpGet("https://example.com/autofarm.lua"))()
    print("自动农场已启动")
end)

-- 重力设置
Slider(PlayerTab, "重力设置", -50, 400, workspace.Gravity, function(value)
    workspace.Gravity = value
    print("重力设置为: " .. value)
end)

print("脚本界面加载完成！")
