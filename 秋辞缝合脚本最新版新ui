SaveManager:IgnoreThemeSettings();
SaveManager:SetIgnoreIndexes({"MenuKeybind"});
SaveManager:SetFolder("Obsidian/秋辞脚本");
ThemeManager:ApplyToTab(Tabs.UISettings);
SaveManager:BuildConfigSection(Tabs.UISettings);
Library.KeybindFrame.Visible = true;
Library:OnKeyPress(function(Key)
	if (Key == Enum.KeyCode.LeftControl) then
		Library:ToggleUI();
	end
end);
print("==================================");
print("秋辞脚本已加载完成！");
print("快捷键: LeftControl");
print("作者: 快手秋辞");
print("副作者: 有芙同享");
print("QQ群: 938714427");
print("==================================");
SaveManager:LoadAutoloadConfig();
game:GetService("Players").PlayerRemoving:Connect(function(Player)
	if (Player == LocalPlayer) then
		local FlatIdent_3B2E6 = 0;
		while true do
			if (3 == FlatIdent_3B2E6) then
				if SpinObject then
					SpinObject:Destroy();
				end
				break;
			end
			if (FlatIdent_3B2E6 == 1) then
				if InfiniteJumpConnection then
					InfiniteJumpConnection:Disconnect();
				end
				if (FlyScript and (type(FlyScript) == "table") and FlyScript.Destroy) then
					pcall(function()
						FlyScript:Destroy();
					end);
				end
				FlatIdent_3B2E6 = 2;
			end
			if (FlatIdent_3B2E6 == 0) then
				if NoclipConnection then
					NoclipConnection:Disconnect();
				end
				if WallWalkConnection then
					WallWalkConnection:Disconnect();
				end
				FlatIdent_3B2E6 = 1;
			end
			if (FlatIdent_3B2E6 == 2) then
				for _, Connection in pairs(ESPConnections) do
					if Connection then
						pcall(function()
							Connection:Disconnect();
						end);
					end
				end
				if ESPFolder then
					ESPFolder:Destroy();
				end
				FlatIdent_3B2E6 = 3;
			end
		end
	end
end);