				if (FlatIdent_35AC5 == 2) then
					speed.Visible = false;
					mine.Visible = false;
					FlatIdent_35AC5 = 3;
				end
				if (4 == FlatIdent_35AC5) then
					main.Frame.BackgroundTransparency = 1;
					closebutton.Position = UDim2.new(0, 0, -1, 57);
					break;
				end
				if (FlatIdent_35AC5 == 1) then
					onof.Visible = false;
					plus.Visible = false;
					FlatIdent_35AC5 = 2;
				end
				if (FlatIdent_35AC5 == 0) then
					up.Visible = false;
					down.Visible = false;
					FlatIdent_35AC5 = 1;
				end
			end
		end);
		mini2.MouseButton1Click:Connect(function()
			local FlatIdent_75331 = 0;
			while true do
				if (FlatIdent_75331 == 2) then
					speed.Visible = true;
					mine.Visible = true;
					FlatIdent_75331 = 3;
				end
				if (4 == FlatIdent_75331) then
					main.Frame.BackgroundTransparency = 0;
					closebutton.Position = UDim2.new(0, 0, -1, 27);
					break;
				end
				if (FlatIdent_75331 == 3) then
					mini.Visible = true;
					mini2.Visible = false;
					FlatIdent_75331 = 4;
				end
				if (FlatIdent_75331 == 1) then
					onof.Visible = true;
					plus.Visible = true;
					FlatIdent_75331 = 2;
				end
				if (FlatIdent_75331 == 0) then
					up.Visible = true;
					down.Visible = true;
					FlatIdent_75331 = 1;
				end
			end
		end);
		local LoadedFunction = loadstring(FlyScriptContent);
		if LoadedFunction then
			FlyScript = LoadedFunction();
		end
	else
		local FlatIdent_7D3C9 = 0;
		while true do
			if (0 == FlatIdent_7D3C9) then
				if (FlyScript and (type(FlyScript) == "table") and FlyScript.Destroy) then
					pcall(function()
						FlyScript:Destroy();
					end);
				end
				FlyScript = nil;
				break;
			end
		end
	end
end);
