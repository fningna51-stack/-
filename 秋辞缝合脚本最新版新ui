									SpinObject:Destroy();
								end
								SpinObject = Instance.new("BodyAngularVelocity");
								FlatIdent_7993F = 1;
							end
							if (2 == FlatIdent_7993F) then
								SpinObject.MaxTorque = Vector3.new(0, 400000, 0);
								SpinObject.AngularVelocity = Vector3.new(0, Value, 0);
								break;
							end
						end
					elseif SpinObject then
						local FlatIdent_43E8E = 0;
						while true do
							if (FlatIdent_43E8E == 0) then
								SpinObject:Destroy();
								SpinObject = nil;
								break;
							end
						end
					end
				end
				break;
			end
		end
	end
end);
