local Players = game:GetService("Players");
local LocalPlayer = Players.LocalPlayer;
local RunService = game:GetService("RunService");
local Lighting = game:GetService("Lighting");
local CoreGui = game:GetService("CoreGui");
local UserInputService = game:GetService("UserInputService");
local TeleportService = game:GetService("TeleportService");
local Workspace = game:GetService("Workspace");
local repo = "https://raw.githubusercontent.com/ATLASTEAM01/Obsidian/main/";
local Library = loadstring(game:HttpGet(repo .. "Library.lua"))();
local ThemeManager = loadstring(game:HttpGet(repo .. "addons/ThemeManager.lua"))();
local SaveManager = loadstring(game:HttpGet(repo .. "addons/SaveManager.lua"))();
local Options = Library.Options;
local Toggles = Library.Toggles;
local Window = Library:CreateWindow({Title="秋辞脚本",SubTitle="作者：快手秋辞 副作者：有芙同享",Center=true,AutoShow=true});
local Tabs = {Universal=Window:AddTab("通用", "user"),Night99=Window:AddTab("99夜", "moon"),DeadRail=Window:AddTab("死铁轨", "train"),Competitor=Window:AddTab("竞争对手", "sword"),InkGame=Window:AddTab("墨水游戏", "paint"),Doors=Window:AddTab("DOORS", "door"),Abandoned=Window:AddTab("被遗弃", "ghost"),jyshoned=Window:AddTab("监狱生活", "jyshjb"),UISettings=Window:AddTab("UI设置", "settings")};
local SpinObject = nil;
local WallWalkEnabled = false;
local NoClipEnabled = false;
local InfiniteJumpEnabled = false;
local FlyEnabled = false;
local NoclipConnection = nil;
local FlyScript = nil;
local WallWalkConnection = nil;
local InfiniteJumpConnection = nil;
local ESPFolder = nil;
local ESPConnections = {};
local NightVisionEnabled = false;
local SafeModeEnabled = true;
local UniversalGroup = Tabs.Universal:AddLeftGroupbox("各种功能");
local UniversalGroupRight = Tabs.Universal:AddRightGroupbox("信息与翻译");
UniversalGroup:AddSlider("WalkSpeedSlider", {Text="移动速度",Default=16,Min=16,Max=200,Rounding=0,Compact=false}):OnChanged(function(Value)
	if (LocalPlayer and LocalPlayer.Character) then
		local Humanoid = LocalPlayer.Character:FindFirstChild("Humanoid");
		if Humanoid then
			Humanoid.WalkSpeed = Value;
		end
	end
end);
UniversalGroup:AddSlider("JumpPowerSlider", {Text="跳跃高度",Default=50,Min=50,Max=200,Rounding=0,Compact=false}):OnChanged(function(Value)
	if (LocalPlayer and LocalPlayer.Character) then
		local FlatIdent_95CAC = 0;
		local Humanoid;
		while true do
			if (FlatIdent_95CAC == 0) then
				Humanoid = LocalPlayer.Character:FindFirstChild("Humanoid");
				if Humanoid then
					Humanoid.JumpPower = Value;
				end
				break;
			end
		end
	end
end);
UniversalGroup:AddInput("JumpPowerInput", {Text="跳跃高度设置",Default="",Placeholder="输入跳跃高度值",Numeric=true,Finished=false,Callback=function(Value)
	local FlatIdent_76979 = 0;
	local numValue;
	while true do
		if (FlatIdent_76979 == 0) then
			numValue = tonumber(Value);
			if (numValue and LocalPlayer and LocalPlayer.Character) then
				local FlatIdent_69270 = 0;
				local Humanoid;
				while true do
					if (FlatIdent_69270 == 0) then
						Humanoid = LocalPlayer.Character:FindFirstChild("Humanoid");
						if Humanoid then
							Humanoid.JumpPower = numValue;
						end
						break;
					end
				end
			end
			break;
		end
	end
end});
UniversalGroup:AddInput("WalkSpeedInput", {Text="移动速度设置",Default="",Placeholder="输入移动速度值",Numeric=true,Finished=false,Callback=function(Value)
	local numValue = tonumber(Value);
	if (numValue and LocalPlayer and LocalPlayer.Character) then
		local Humanoid = LocalPlayer.Character:FindFirstChild("Humanoid");
		if Humanoid then
			Humanoid.WalkSpeed = numValue;
		end
	end
end});
UniversalGroup:AddInput("GravityInput", {Text="重力设置",Default="",Placeholder="输入重力值",Numeric=true,Finished=false,Callback=function(Value)
	local FlatIdent_6D4CB = 0;
	local numValue;
	while true do
		if (FlatIdent_6D4CB == 0) then
			numValue = tonumber(Value);
			if numValue then
				Workspace.Gravity = numValue;
			end
			break;
		end
	end
end});
UniversalGroup:AddToggle("NightVisionToggle", {Text="夜视",Default=false}):OnChanged(function(Value)
	local FlatIdent_10BCC = 0;
	while true do
		if (0 == FlatIdent_10BCC) then
			NightVisionEnabled = Value;
			if Value then
				local FlatIdent_47A9C = 0;
				while true do
					if (FlatIdent_47A9C == 1) then
						Lighting.OutdoorAmbient = Color3.new(1, 1, 1);
						Lighting.FogEnd = 100000;
						FlatIdent_47A9C = 2;
					end
					if (FlatIdent_47A9C == 2) then
						Lighting.FogStart = 0;
						Lighting.GlobalShadows = false;
						break;
					end
					if (FlatIdent_47A9C == 0) then
						Lighting.Ambient = Color3.new(1, 1, 1);
						Lighting.Brightness = 1;
						FlatIdent_47A9C = 1;
					end
				end
			else
				Lighting.Ambient = Color3.new(0, 0, 0);
				Lighting.Brightness = 1;
				Lighting.OutdoorAmbient = Color3.new(0.5, 0.5, 0.5);
				Lighting.FogEnd = 100000;
				Lighting.FogStart = 0;
				Lighting.GlobalShadows = true;
			end
			break;
		end
	end
end);
UniversalGroup:AddToggle("ESPToggle", {Text="透视",Default=false}):OnChanged(function(Value)
	if Value then
		if not ESPFolder then
			local FlatIdent_60EA1 = 0;
			while true do
				if (FlatIdent_60EA1 == 1) then
					ESPFolder.Parent = CoreGui;
					break;
				end
				if (FlatIdent_60EA1 == 0) then
					ESPFolder = Instance.new("Folder");
					ESPFolder.Name = "Highlight_Storage";
					FlatIdent_60EA1 = 1;
				end
			end
		end
		local FillColor = Color3.fromRGB(175, 25, 255);
		local OutlineColor = Color3.fromRGB(255, 255, 255);
		for _, Player in pairs(Players:GetPlayers()) do
			if (Player ~= LocalPlayer) then
				local Highlight = Instance.new("Highlight");
				Highlight.Name = Player.Name;
				Highlight.FillColor = FillColor;
				Highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop;
				Highlight.FillTransparency = 0.5;
				Highlight.OutlineColor = OutlineColor;
				Highlight.OutlineTransparency = 0;
				Highlight.Parent = ESPFolder;
				local function UpdateAdornee(Character)
					if Character then
						Highlight.Adornee = Character;
					end
				end
				if Player.Character then
					UpdateAdornee(Player.Character);
				end
				local CharacterConnection = Player.CharacterAdded:Connect(UpdateAdornee);
				ESPConnections[Player] = CharacterConnection;
			end
		end
		local PlayerAddedConnection = Players.PlayerAdded:Connect(function(Player)
			local Highlight = Instance.new("Highlight");
			Highlight.Name = Player.Name;
			Highlight.FillColor = FillColor;
			Highlight.DepthMode = Enum.HighlightDepthMode.AlwaysOnTop;
			Highlight.FillTransparency = 0.5;
			Highlight.OutlineColor = OutlineColor;
			Highlight.OutlineTransparency = 0;
			Highlight.Parent = ESPFolder;
			local function UpdateAdornee(Character)
				if Character then
					Highlight.Adornee = Character;
				end
			end
			if Player.Character then
				UpdateAdornee(Player.Character);
			end
			local CharacterConnection = Player.CharacterAdded:Connect(UpdateAdornee);
			ESPConnections[Player] = CharacterConnection;
		end);
		local PlayerRemovingConnection = Players.PlayerRemoving:Connect(function(Player)
			local FlatIdent_8F047 = 0;
			while true do
				if (FlatIdent_8F047 == 0) then
					if ESPConnections[Player] then
						local FlatIdent_31905 = 0;
						while true do
							if (0 == FlatIdent_31905) then
								ESPConnections[Player]:Disconnect();
								ESPConnections[Player] = nil;
								break;
							end
						end
					end
					if ESPFolder then
						local FlatIdent_51F42 = 0;
						local PlayerHighlight;
						while true do
							if (FlatIdent_51F42 == 0) then
								PlayerHighlight = ESPFolder:FindFirstChild(Player.Name);
								if PlayerHighlight then
									PlayerHighlight:Destroy();
								end
								break;
							end
						end
					end
					break;
				end
			end
		end);
		ESPConnections['PlayerAdded'] = PlayerAddedConnection;
		ESPConnections['PlayerRemoving'] = PlayerRemovingConnection;
	else
		for _, Connection in pairs(ESPConnections) do
			if Connection then
				pcall(function()
					Connection:Disconnect();
				end);
			end
		end
		ESPConnections = {};
		if ESPFolder then
			ESPFolder:Destroy();
			ESPFolder = nil;
		end
	end
end);
UniversalGroup:AddButton("墨水飞行", function()
	local FlyModule = {};
	FlyModule.Config = {MoveSpeed=10,FreezeEnabled=false,FreezeY=nil};
	FlyModule.State = {UI=nil,Connections={},Initialized=false};
	FlyModule.Init = function()
		local FlatIdent_1BCFB = 0;
		while true do
			if (FlatIdent_1BCFB == 3) then
				FlyModule.State.Initialized = true;
				break;
			end
			if (2 == FlatIdent_1BCFB) then
				FlyModule.Humanoid = nil;
				FlyModule.HRP = nil;
				FlatIdent_1BCFB = 3;
			end
			if (FlatIdent_1BCFB == 1) then
				FlyModule.Player = FlyModule.Services.Players.LocalPlayer;
				FlyModule.Character = nil;
				FlatIdent_1BCFB = 2;
			end
			if (FlatIdent_1BCFB == 0) then
				if FlyModule.State.Initialized then
					return;
				end
				FlyModule.Services = {Players=game:GetService("Players"),UserInputService=game:GetService("UserInputService"),RunService=game:GetService("RunService"),CoreGui=game:GetService("CoreGui")};
				FlatIdent_1BCFB = 1;
			end
		end
	end;
	FlyModule.CreateUI = function()
		local FlatIdent_49AED = 0;
		local ui;
		local speedInput;
		local freezeBtn;
		while true do
			if (FlatIdent_49AED == 5) then
				freezeBtn.MouseButton1Click:Connect(function()
					FlyModule.Config.FreezeEnabled = not FlyModule.Config.FreezeEnabled;
					freezeBtn.Text = (FlyModule.Config.FreezeEnabled and "悬挂√") or "开启空中悬挂";
					freezeBtn.BackgroundColor3 = (FlyModule.Config.FreezeEnabled and Color3.fromRGB(255, 200, 200)) or Color3.fromRGB(210, 240, 255);
					if not FlyModule.Config.FreezeEnabled then
						FlyModule.Config.FreezeY = nil;
					end
				end);
				FlyModule.State.UI = ui;
				return ui;
			end
			if (FlatIdent_49AED == 2) then
				speedInput.TextSize = 18;
				speedInput.PlaceholderText = "移动速度";
				speedInput.BackgroundColor3 = Color3.fromRGB(255, 255, 255);
				speedInput.BackgroundTransparency = 0.2;
				speedInput.TextColor3 = Color3.fromRGB(0, 0, 0);
				Instance.new("UICorner").Parent = speedInput;
				FlatIdent_49AED = 3;
			end
			if (FlatIdent_49AED == 3) then
				speedInput.FocusLost:Connect(function()
					local FlatIdent_74348 = 0;
					local v;
					while true do
						if (FlatIdent_74348 == 0) then
							v = tonumber(speedInput.Text);
							if (v and (v > 0)) then
								FlyModule.Config.MoveSpeed = v;
							else
								speedInput.Text = tostring(FlyModule.Config.MoveSpeed);
							end
							break;
						end
					end
				end);
				freezeBtn = Instance.new("TextButton");
				freezeBtn.Name = "FreezeBtn";
				freezeBtn.Parent = ui;
				freezeBtn.Size = UDim2.new(0, 120, 0, 38);
				freezeBtn.Position = UDim2.new(0.95, -120, 0.05, 0);
				FlatIdent_49AED = 4;
			end
			if (FlatIdent_49AED == 4) then
				freezeBtn.Text = "开启空中悬挂";
				freezeBtn.TextSize = 20;
				freezeBtn.BackgroundColor3 = Color3.fromRGB(210, 240, 255);
				freezeBtn.BackgroundTransparency = 0.2;
				freezeBtn.TextColor3 = Color3.fromRGB(0, 0, 0);
				Instance.new("UICorner").Parent = freezeBtn;
				FlatIdent_49AED = 5;
			end
			if (FlatIdent_49AED == 0) then
				if FlyModule.State.UI then
					local FlatIdent_5BA5E = 0;
					while true do
						if (FlatIdent_5BA5E == 0) then
							FlyModule.State.UI:Destroy();
							FlyModule.State.UI = nil;
							break;
						end
					end
				end
				ui = Instance.new("ScreenGui");
				ui.Name = "QC_FlyUI";
				ui.Parent = FlyModule.Services.CoreGui;
				ui.DisplayOrder = 2000;
				ui.ResetOnSpawn = false;
				FlatIdent_49AED = 1;
			end
			if (FlatIdent_49AED == 1) then
				speedInput = Instance.new("TextBox");
				speedInput.Name = "SpeedInput";
				speedInput.Parent = ui;
				speedInput.Size = UDim2.new(0, 120, 0, 30);
				speedInput.Position = UDim2.new(0.95, -120, 0.05, 38);
				speedInput.Text = tostring(FlyModule.Config.MoveSpeed);
				FlatIdent_49AED = 2;
			end
		end
	end;
	FlyModule.SetupCharacter = function(char)
		local FlatIdent_759F1 = 0;
		while true do
			if (FlatIdent_759F1 == 1) then
				FlyModule.HRP = char:WaitForChild("HumanoidRootPart", 10);
				FlyModule.Humanoid.Died:Connect(function()
					local FlatIdent_6B983 = 0;
					while true do
						if (FlatIdent_6B983 == 1) then
							if FlyModule.State.UI then
								local FlatIdent_287B5 = 0;
								local freezeBtn;
								while true do
									if (FlatIdent_287B5 == 0) then
										freezeBtn = FlyModule.State.UI:FindFirstChild("FreezeBtn");
										if freezeBtn then
											local FlatIdent_4CC24 = 0;
											while true do
												if (FlatIdent_4CC24 == 0) then
													freezeBtn.Text = "开启空中悬挂";
													freezeBtn.BackgroundColor3 = Color3.fromRGB(210, 240, 255);
													break;
												end
											end
										end
										break;
									end
								end
							end
							break;
						end
						if (FlatIdent_6B983 == 0) then
							FlyModule.Config.FreezeEnabled = false;
							FlyModule.Config.FreezeY = nil;
							FlatIdent_6B983 = 1;
						end
					end
				end);
				break;
			end
			if (0 == FlatIdent_759F1) then
				FlyModule.Character = char;
				FlyModule.Humanoid = char:WaitForChild("Humanoid", 10);
				FlatIdent_759F1 = 1;
			end
		end
	end;
	FlyModule.SetupInfiniteJump = function()
		table.insert(FlyModule.State.Connections, FlyModule.Services.UserInputService.JumpRequest:Connect(function()
			if (FlyModule.Humanoid and (FlyModule.Humanoid:GetState() ~= Enum.HumanoidStateType.Dead)) then
				FlyModule.Humanoid:ChangeState(Enum.HumanoidStateType.Jumping);
			end
		end));
	end;
	FlyModule.SetupMainLoop = function()
		table.insert(FlyModule.State.Connections, FlyModule.Services.RunService.Heartbeat:Connect(function(dt)
			if (FlyModule.Humanoid and FlyModule.HRP and (FlyModule.Humanoid:GetState() ~= Enum.HumanoidStateType.Dead)) then
				local dir = FlyModule.Humanoid.MoveDirection;
				if FlyModule.Config.FreezeEnabled then
					local FlatIdent_5346B = 0;
					local state;
					while true do
						if (FlatIdent_5346B == 0) then
							state = FlyModule.Humanoid:GetState();
							if ((state == Enum.HumanoidStateType.Freefall) and (FlyModule.Humanoid:GetState() ~= Enum.HumanoidStateType.Jumping)) then
								if (dir.Magnitude > 0) then
									if not FlyModule.Config.FreezeY then
										FlyModule.Config.FreezeY = FlyModule.HRP.Position.Y;
									end
									local moveXZ = Vector3.new(dir.X, 0, dir.Z) * FlyModule.Config.MoveSpeed * dt;
									local pos = FlyModule.HRP.Position;
									local newPos = Vector3.new(pos.X, FlyModule.Config.FreezeY, pos.Z) + moveXZ;
									FlyModule.HRP.CFrame = CFrame.new(newPos, newPos + FlyModule.HRP.CFrame.LookVector);
									FlyModule.HRP.Velocity = Vector3.new(0, 0, 0);
								else
									FlyModule.Config.FreezeY = nil;
								end
								return;
							else
								FlyModule.Config.FreezeY = nil;
							end
							break;
						end
					end
				end
				if (dir.Magnitude > 0) then
					FlyModule.HRP.CFrame = FlyModule.HRP.CFrame + (dir.Unit * FlyModule.Config.MoveSpeed * dt);
				end
			end
		end));
	end;
	FlyModule.SetupCharacterListeners = function()
		local FlatIdent_28F1 = 0;
		while true do
			if (0 == FlatIdent_28F1) then
				if FlyModule.Player.Character then
					FlyModule.SetupCharacter(FlyModule.Player.Character);
				end
				table.insert(FlyModule.State.Connections, FlyModule.Player.CharacterAdded:Connect(function(char)
					FlyModule.SetupCharacter(char);
				end));
				FlatIdent_28F1 = 1;
			end
			if (1 == FlatIdent_28F1) then
				table.insert(FlyModule.State.Connections, FlyModule.Player.CharacterRemoving:Connect(function()
					local FlatIdent_5477B = 0;
					while true do
						if (FlatIdent_5477B == 0) then
							FlyModule.Character = nil;
							FlyModule.Humanoid = nil;
							FlatIdent_5477B = 1;
						end
						if (1 == FlatIdent_5477B) then
							FlyModule.HRP = nil;
							FlyModule.Config.FreezeEnabled = false;
							FlatIdent_5477B = 2;
						end
						if (FlatIdent_5477B == 2) then
							FlyModule.Config.FreezeY = nil;
							break;
						end
					end
				end));
				break;
			end
		end
	end;
	FlyModule.Start = function()
		local FlatIdent_2E9CB = 0;
		while true do
			if (FlatIdent_2E9CB == 1) then
				FlyModule.SetupInfiniteJump();
				FlyModule.SetupMainLoop();
				FlatIdent_2E9CB = 2;
			end
			if (FlatIdent_2E9CB == 0) then
				FlyModule.Init();
				FlyModule.CreateUI();
				FlatIdent_2E9CB = 1;
			end
			if (FlatIdent_2E9CB == 2) then
				FlyModule.SetupCharacterListeners();
				break;
			end
		end
	end;
	FlyModule.Stop = function()
		local FlatIdent_69253 = 0;
		while true do
			if (FlatIdent_69253 == 3) then
				FlyModule.Humanoid = nil;
				FlyModule.HRP = nil;
				break;
			end
			if (1 == FlatIdent_69253) then
				if FlyModule.State.UI then
					local FlatIdent_DFF4 = 0;
					while true do
						if (FlatIdent_DFF4 == 0) then
							FlyModule.State.UI:Destroy();
							FlyModule.State.UI = nil;
							break;
						end
					end
				end
				FlyModule.Config.FreezeEnabled = false;
				FlatIdent_69253 = 2;
			end
			if (FlatIdent_69253 == 2) then
				FlyModule.Config.FreezeY = nil;
				FlyModule.Character = nil;
				FlatIdent_69253 = 3;
			end
			if (0 == FlatIdent_69253) then
				for _, conn in pairs(FlyModule.State.Connections) do
					conn:Disconnect();
				end
				FlyModule.State.Connections = {};
				FlatIdent_69253 = 1;
			end
		end
	end;
	FlyModule.GetInfo = function()
		return {Name="飞行悬挂模块",Version="1.0",Author="QC Script",Description="提供飞行、悬挂、无限跳跃功能"};
	end;
	FlyModule.Start();
end);
UniversalGroup:AddButton("无限跳", function()
	local FlatIdent_7DFA5 = 0;
	while true do
		if (FlatIdent_7DFA5 == 1) then
			if InfiniteJumpEnabled then
				InfiniteJumpConnection = UserInputService.JumpRequest:Connect(function()
					if LocalPlayer.Character then
						local FlatIdent_72421 = 0;
						local Humanoid;
						while true do
							if (FlatIdent_72421 == 0) then
								Humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid");
								if Humanoid then
									Humanoid:ChangeState("Jumping");
								end
								break;
							end
						end
					end
				end);
			end
			break;
		end
		if (FlatIdent_7DFA5 == 0) then
			InfiniteJumpEnabled = not InfiniteJumpEnabled;
			if InfiniteJumpConnection then
				local FlatIdent_25A9F = 0;
				while true do
					if (FlatIdent_25A9F == 0) then
						InfiniteJumpConnection:Disconnect();
						InfiniteJumpConnection = nil;
						break;
					end
				end
			end
			FlatIdent_7DFA5 = 1;
		end
	end
end);
UniversalGroup:AddButton("爬墙", function()
	local FlatIdent_4508F = 0;
	while true do
		if (FlatIdent_4508F == 1) then
			if WallWalkEnabled then
				WallWalkConnection = RunService.Heartbeat:Connect(function()
					if LocalPlayer.Character then
						local FlatIdent_77172 = 0;
						local Humanoid;
						local HumanoidRootPart;
						while true do
							if (FlatIdent_77172 == 1) then
								if (Humanoid and HumanoidRootPart) then
									local FlatIdent_7E707 = 0;
									local Ray;
									local Hit;
									local Position;
									while true do
										if (FlatIdent_7E707 == 1) then
											if Hit then
												Humanoid:ChangeState(Enum.HumanoidStateType.Climbing);
												HumanoidRootPart.CFrame = CFrame.new(Position) * CFrame.new(0, 3, 0);
											end
											break;
										end
										if (FlatIdent_7E707 == 0) then
											Ray = Ray.new(HumanoidRootPart.Position, HumanoidRootPart.CFrame.LookVector * 5);
											Hit, Position = Workspace:FindPartOnRay(Ray, LocalPlayer.Character);
											FlatIdent_7E707 = 1;
										end
									end
								end
								break;
							end
							if (FlatIdent_77172 == 0) then
								Humanoid = LocalPlayer.Character:FindFirstChildOfClass("Humanoid");
								HumanoidRootPart = LocalPlayer.Character:FindFirstChild("HumanoidRootPart");
								FlatIdent_77172 = 1;
							end
						end
					end
				end);
			end
			break;
		end
		if (FlatIdent_4508F == 0) then
			WallWalkEnabled = not WallWalkEnabled;
			if WallWalkConnection then
				local FlatIdent_1013A = 0;
				while true do
					if (0 == FlatIdent_1013A) then
						WallWalkConnection:Disconnect();
						WallWalkConnection = nil;
						break;
					end
				end
			end
			FlatIdent_4508F = 1;
		end
	end
end);
